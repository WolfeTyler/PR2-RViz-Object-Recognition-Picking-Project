    <limit effort="10000" velocity="0.1"/>
    <child link="base_footprint" />
    <parent link="world"/>
  </joint>
  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 0.051" rpy="0 0 0" />
